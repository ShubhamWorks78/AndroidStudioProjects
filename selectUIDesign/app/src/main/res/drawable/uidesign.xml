<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="false"

        >
        <shape
            android:shape="oval">
            <stroke
              android:color="#900"
                android:width="10dp">

            </stroke>
            <gradient
                android:startColor="#345"
                android:centerColor="#456"
                android:endColor="#123"
                >

            </gradient>
            <corners
                android:radius="10dp"
                ></corners>
        </shape>
    </item>


</selector>