<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.seals.shubham.ecommerce"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ForgetPass">
        </activity>
        <activity android:name=".RegistrationActivity">
        </activity>
        <activity
            android:name=".UserPage"
            android:label="@string/title_activity_user_page"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".AdminPage"
            android:label="@string/title_activity_admin_page"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Books">
        </activity>
        <activity android:name=".Clothing">
        </activity>
        <activity android:name=".MobilesAndTablets">
        </activity>
        <activity android:name=".Computer">
        </activity>
        <activity android:name=".TravelLuggage">
        </activity>
        <activity android:name=".TvAndAudio">
        </activity>
        <activity android:name=".HomeNdKitchen">
        </activity>
        <activity android:name=".AutoMotive">
        </activity>
        <activity android:name=".FootWear">
        </activity>
    </application>

</manifest>