<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.seals.shubham.quizwithsql"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.SEND_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/eve"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegistrationActivity">
        </activity>
        <activity android:name=".ForgetPass">
        </activity>
        <activity android:name=".SendSmsActivity">
        </activity>
        <activity android:name=".ResetPass">
        </activity>
        <activity android:name=".AddQuestions">
        </activity>
        <activity android:name=".AdminPage">
        </activity>
        <activity android:name=".UserPage">
        </activity>
        <activity android:name=".UpdateQuestions">
        </activity>
        <activity android:name=".HighScores">
        </activity>
        <activity android:name=".Show_Questions">
        </activity>
        <activity android:name=".ShowResults">
        </activity>
        <activity android:name=".ShowAllQuestion">
        </activity>
        <activity android:name=".finalQues">
        </activity>
        <activity android:name=".ShowSingleQuest">
        </activity>
        <activity android:name=".FinalResults">
        </activity>
        <activity android:name=".EditAns">
        </activity>
        <activity android:name=".AnsListView">
        </activity>
    </application>

</manifest>